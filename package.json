{
  "name": "crawler-bun",
  "version": "1.0.0",
  "description": "A TypeScript-based web crawler CLI built with Bun.",
  "type": "module",
  "module": "index.ts",
  "main": "index.ts",
  "bin": {
    "crawler-bun": "./index.ts"
  },
  "scripts": {
    "start": "bun run index.ts",
    "dev": "bun --watch index.ts",
    "build": "tsc"
  },
  "private": false,
  "license": "MIT",
  "dependencies": {
    "cheerio": "^1.1.2",
    "robots-parser": "^3.0.1"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/NalinDalal/crawler-bun.git"
  },
  "keywords": [
    "webcrawler",
    "bun",
    "typescript",
    "cli",
    "crawler"
  ],
  "author": "Nalin Dalal"
}
